{
    "name": "happyr/auth0-bundle",
    "type": "symfony-bundle",
    "description": "Symfony integration with auth0",
    "license": "MIT",
    "authors": [
        {
            "name": "Tobias Nyholm",
            "email": "tobias.nyholm@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "auth0/auth0-php": "dev-updated-deps-reqs",
        "firebase/php-jwt": "^4.0",
        "php-http/cache-plugin": "^1.3",
        "php-http/client-common": "^1.5|^2.0",
        "php-http/discovery": "^1.6.1",
        "php-http/httplug": "^1.0|^2.0",
        "php-http/message": "^1.7.2",
        "psr/cache": "~1.0",
        "psr/log": "~1.0",
        "symfony/config": "^2.8|^3.0|^4.0|^5.0",
        "symfony/dependency-injection": "^2.8|^3.0|^4.0|^5.0",
        "symfony/event-dispatcher": "^2.8|^3.0|^4.0|^5.0",
        "symfony/framework-bundle": "^2.8|^3.0|^4.0|^5.0",
        "symfony/http-foundation": "^2.8|^3.0|^4.0|^5.0",
        "symfony/http-kernel": "^2.8|^3.0|^4.0|^5.0",
        "symfony/security-bundle": "^2.8|^3.0|^4.0|^5.0",
        "symfony/security-core": "^2.8|^3.0|^4.0|^5.0",
        "symfony/security-http": "^2.8|^3.0|^4.0|^5.0",
        "symfony/security-csrf": "^2.8|^3.0|^4.0|^5.0",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "guzzlehttp/psr7": "^1.3",
        "matthiasnoback/symfony-dependency-injection-test": "^4.1.1",
        "nyholm/symfony-bundle-test": "^1.2",
        "php-http/curl-client": "^1.6",
        "php-http/message-factory": "^1.0.2",
        "php-http/mock-client": "^1.0",
        "phpunit/phpunit": "^5.5|^6.2|^8.0"
    },
    "autoload": {
        "psr-4": { "Happyr\\Auth0Bundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-ci": "vendor/bin/phpunit --coverage-text --coverage-clover=build/coverage.xml"
    },
    "repositories": [
        {"type": "composer", "url": "https://repo.packagist.com/rdey/"},
        {"packagist.org": false}
    ]
}
